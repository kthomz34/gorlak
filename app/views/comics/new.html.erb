<div class="booyah-box col-xs-10 col-xs-offset-1">
  <h1>Enter Your Comic Details</h1>
  <%= simple_form_for @comics do |f| %>
    <%= f.input :name %>
    <%= f.input :artist %>
    <%= f.input :description %>
    <br />
    <%= f.submit 'Create', :class => 'btn btn-primary' %>
  <% end %>
</div>